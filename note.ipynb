    "import numpy as np\n",
    "\n",
    "a = np.array([[1,2],[3,4]])\n",
    "b = np.array([[2],[2]])\n",
    "\n",
    "# 转置\n",
    "print (a.T)\n",
    "\n",
    "# 求逆\n",
    "print (np.linalg.inv(a))\n",
    "\n",
    "# 矩阵相乘\n",
    "print(a @ b)"
